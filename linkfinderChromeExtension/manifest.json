{
  "manifest_version": 3,
  "name": "Get all links",
  "version": "1.0",
  "description": "Extracts script paths from the current website and displays them in a small popup.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}

